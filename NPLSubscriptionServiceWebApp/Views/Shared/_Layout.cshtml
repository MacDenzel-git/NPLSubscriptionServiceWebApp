</head>
<body class="">
    <div class="wrapper">
        <div class="sidebar" data-color="navy" data-background-color="white" data-image="img/sidebar-1.jpg">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="purple | azure | green | orange | danger | navy"

                Tip 2: you can also add an image using data-image tag
            -->
            <div class="logo">
                <a asp-controller="Home" class="simple-text logo-normal">
                    <img src="/img/TN.jpg" width="90%" height="75px" /><br></a>
                 </a>
              
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav pb-4">
                    <li class="nav-item">
                        <a class="p-2 nav-link mr-0" asp-controller="Client" asp-action="Index">
                            <i class="p-2 material-icons">folder</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Clients</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="p-2 nav-link mr-0" asp-controller="Promotion" asp-action="Index">
                            <i class="p-2 material-icons">folder</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Promotion</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="p-2 nav-link mr-0" asp-controller="Payment" asp-action="Index">
                            <i class="p-2 material-icons">folder</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Payment</p>
                        </a>
                    </li>
                     <li class="nav-item">
                        <a class="p-2 nav-link mr-0" asp-controller="Subscriptions" asp-action="Index">
                            <i class="p-2 material-icons">folder</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Subscriptions</p>
                        </a>
                    </li>
                    <li class="pt-3 header-nav text-center font-weight-bold">Settings</li>
                    
                    <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="ClientType" asp-action="Index">
                            <i class="p-2 material-icons">inventory_2</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Client Type</p>
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="District" asp-action="Index">
                            <i class="p-2 material-icons">inventory_2</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">District</p>
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="Region" asp-action="Index">
                            <i class="p-2 material-icons">inventory_2</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Region</p>
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="PaymentType" asp-action="Index">
                            <i class="p-2 material-icons">category</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Payment Type </p>
                        </a>
                    </li>
                     <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="SubscriptionType" asp-action="Index">
                            <i class="p-2 material-icons">category</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Subscription Type </p>
                        </a>
                    </li>
                     <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="SubscriptionStatus" asp-action="Index">
                            <i class="p-2 material-icons">category</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Subscription Status </p>
                        </a>
                    </li>
                         <li class="pt-3 header-nav text-center font-weight-bold">Security</li>
                    <li class="nav-item ">
                        <a class="p-2 nav-link mr-0" asp-controller="Company" asp-action="Index">
                            <i class="p-2 material-icons">people</i>
                            <p class="p-2" style="box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.14), 0 2px 2px -4px rgba(0, 0, 0, 0.4);">Users - WIP!!</p>
                        </a>
                     </li>
                </ul>
            </div>
        </div>

        <div class="main-panel">

            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <a class="navbar-brand" href="#pablo">

                        </a>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end">
                        <ul class="navbar-nav">

                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#pablo" id="navbarDropdownProfile" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">person</i>
                                    <p class="d-lg-none d-md-block">
                                        Account
                                    </p>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownProfile">
                                    <a class="dropdown-item" asp-controller="User" asp-action="Index">Profile</a>
                                    <div class="dropdown-divider"></div>
                                    <a asp-action="Logout" asp-controller="Account" class="dropdown-item">Log out</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->

            <div class="content">
                <div class="container">
                    @*<partial name="_CookieConsentPartial" />*@
                    <main role="main" class="pb-3">

                        @RenderBody()
                    </main>
                </div>
            </div>

            <footer class="footer">
                <div class="container-fluid">
                    <div class="copyright float-right">
                        &copy;
                        <script>
                            document.write(new Date().getFullYear())
                        </script>
                        <a href="https://www.techart.com" target="_blank">Submitted By Denzel Machowa</a>.
                    </div>
                </div>
            </footer>
        </div>

    </div>

    @*Script Section*@
    <script src="~/js/core/jquery.min.js"></script>
    <script src="~/js/core/popper.min.js"></script>
    <script src="~/js/core/bootstrap-material-design.min.js"></script>
